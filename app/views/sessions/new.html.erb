<h1> Log In </h1>

<%= form_with(scope: :session, url: login_path, local: true) do |f| %>

    <p>
        <%= f.label :email %>
        <%= f.text_field :email %>
    </p>

    <p> 
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>

    <%= f.submit 'LogIn' %>

<% end %>
<br><br>
<%= link_to "<button>SignUp</button>".html_safe, signup_path%>